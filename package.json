{
  "name": "@capacitor-community/tapsellplus",
  "version": "1.0.0",
  "type": "module",
  "description": "Capacitor plugin for TapsellPlus ads SDK",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/plugin.cjs.js",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "unpkg": "dist/plugin.js",
  "private": false,
  "files": [
    "android/",
    "dist/",
    "ios/",
    "TapsellPlusPlugin.podspec"
  ],
  "author": "smrsan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smrsan/capacitor-tapsellplus-sdk"
  },
  "bugs": {
    "url": "https://github.com/smrsan/capacitor-tapsellplus-sdk/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "tapsell",
    "tapsellplus",
    "ads",
    "advertising"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "typedoc --out docs src",
    "build": "npm run clean && tsc && npm run bundle",
    "bundle": "rollup -c rollup.config.js",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.0.0",
    "prettier-plugin-java": "^1.0.0",
    "swiftlint": "^1.0.1",
    "typedoc": "^0.28.0",
    "typescript": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rollup": "^3.0.0"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ]
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
